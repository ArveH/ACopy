<?xml version="1.0" encoding="utf-8"?>
<!--Default type conversion file for ACopy (Oracle writer)-->
<TypeConversions From="Oracle" To="Default">
  <Type Source="binary_float" Destination="binaryfloat"></Type>
  <Type Source="binary_double" Destination="binarydouble"></Type>
  <Type Source="blob" Destination="blob"></Type>
  <Type Source="char(@Length)" Destination="char(@Length)"/>
  <Type Source="clob" Destination="longtext"/>
  <Type Source="date" Destination="datetime"></Type>
  <Type Source="float(@Prec)" Destination="float(@Prec)"></Type>
<!--ANSI types int, integer and smallint are all created as NUMBER without prec and scale-->
  <Type Source="long" Destination="oldtext"></Type>
  <Type Source="longraw" Destination="oldblob"></Type>
  <Type Source="nchar(@Length)" Destination="nchar(@Length)"/>
  <Type Source="nclob" Destination="nlongtext"/>
  <Type Source="number(@Prec,@Scale)" Destination="dec(@Prec,@Scale)"></Type>
  <Type Source="nvarchar2(@Length)" Destination="nvarchar(@Length)"/>
  <!--<Type Source="raw(@Length)" Destination="guid">
    <Length Operator="in">16,32</Length>
  </Type>-->
  <Type Source="raw(@Length)" Destination="raw(@Length)"></Type>
<!--ANSI type real is created as FLOAT(63)-->
  <Type Source="timestamp(@Scale)" Destination="datetime2(@Scale)"></Type>
  <Type Source="varchar2(@Length)" Destination="varchar(@Length)"/>
<!--ANSI type varchar is created as VARCHAR2-->
</TypeConversions>
