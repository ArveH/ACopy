<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <connectionStrings>
    <add name="aw" connectionString="Trusted_Connection=True;database=AdventureWorks;server=(local)"/>
  </connectionStrings>
  <commandLineParameters>
    <!--
    For copy in only (-din, FromTileToTable). 
    Collects rows in a "batch", and sends the batch to the server as one operation.
    SQL Server: Default is 0 (zero), which means that we send for every row.
    Oracle: Default is 10000.-->
  </commandLineParameters>

  <configurationParameters>
    <!--
    For copy in only (-din, FromTileToTable). 
    Collects rows in a "batch", and sends the batch to the server as one operation.
    SQL Server: Default is 0 (zero), which means that we send for every row.
    Oracle: Default is 10000.-->
    <!--<batchSize value=10000/>-->

    <!--
    Sql Server only. 
    For copy in only (-din, FromTileToTable). 
    Override the default database collation. If no collation is given, it will use the 
    collation it finds in the datafile (the collation from the database it was copied from).-->
    <!--<collation value="SQL_Latin1_General_CP1_CI_AI"/>-->

    <!--
    Default suffix for a data file is 'adata', but you can set it to what you want. 
    OBS: Remember to use the same value for copy in, as copy out.-->
    <!--<data_suffix value="adata"/>-->

    <!--
    Default suffix for a schema file is 'aschema', but you can set it to what you want. 
    OBS: Remember to use the same value for copy in, as copy out.-->
    <!--<schema_suffix value="aschema"/>-->

  </configurationParameters>

  <conversionFiles>
    <oracleFromFileToTable value="ConversionFiles\OracleReaderConversions.xml"/>
    <oracleFromTableToFile value="ConversionFiles\OracleWriterConversions.xml"/>
    <sqlServerFromFileToTable value="ConversionFiles\SqlServerReaderConversions.xml"/>
    <sqlServerFromTableToFile value="ConversionFiles\SqlServerWriterConversions.xml"/>
  </conversionFiles>
</configuration>