<?xml version="1.0" encoding="utf-8"?>
<!--Default type conversion file for ACopy (Oracle Reader)-->
<TypeConversions From="Default" To="Oracle">
  <Type Source="blob" Destination="varbinary(-1)"/>
  <Type Source="bool" Destination="bit"/>
  <Type Source="char(@Length)" Destination="char(@Length)"/>
  <Type Source="date" Destination="date"/>
  <Type Source="datetime" Destination="datetime"/>
  <Type Source="datetime2" Destination="datetime2"/>
  <Type Source="dec(@Prec,@Scale)" Destination="dec(@Prec,@Scale)"/>
  <Type Source="double" Destination="float"/>
  <Type Source="float" Destination="float"/>
  <Type Source="guid" Destination="uniqueidentity"/>
  <Type Source="identity(@Length)" Destination="dec(@Length,0)"/>
  <Type Source="int" Destination="int"/>
  <Type Source="int16" Destination="smallint"/>
  <Type Source="int64" Destination="bigint"/>
  <Type Source="int8" Destination="tinyint"/>
  <Type Source="longtext" Destination="varchar(-1)"/>
  <Type Source="money" Destination="money"/>
  <Type Source="nchar(@Length)" Destination="nchar(@Length)"/>
  <Type Source="nlongtext" Destination="nvarchar(-1)"/>
  <Type Source="nvarchar(@Length)" Destination="nvarchar(@Length)"/>
  <Type Source="raw(@Length)" Destination="varbinary(@Length)"/>
  <Type Source="varchar(@Length)" Destination="varchar(@Length)"/>
</TypeConversions>
