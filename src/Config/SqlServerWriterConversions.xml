<?xml version="1.0" encoding="utf-8"?>
<!--Default type conversion file for ACopy (SqlServer writer)-->
<TypeConversions From="SqlServer" To="Default">
  <Type Source="bigint" Destination="int64"></Type>
  <Type Source="binary(@scale)" Destination="raw(@Length)"></Type>
  <Type Source="bit" Destination="bool"></Type>
  <Type Source="char(@Length)" Destination="char(@Length)"></Type>
  <Type Source="date" Destination="date"></Type>
  <Type Source="datetime" Destination="datetime"></Type>
  <Type Source="datetime2(@Scale)" Destination="datetime2(@Scale)"></Type>
  <Type Source="decimal(@Prec,@Scale)" Destination="dec(@Prec,@Scale)"></Type>
  <Type Source="numeric(@Prec,@Scale)" Destination="dec(@Prec,@Scale)"></Type>
  <Type Source="float(@Prec)" Destination="binaryfloat">
      <Prec Operator="=">24</Prec>
  </Type>
  <Type Source="float(@Prec)" Destination="binarydouble">
      <Prec Operator="=">53</Prec>
  </Type>
  <Type Source="float(@Prec)" Destination="float(@Prec)"></Type>
  <Type Source="image" Destination="oldblob"></Type>
  <Type Source="int" Destination="int"></Type>
  <Type Source="money" Destination="money"></Type>
  <Type Source="nchar(@Length)" Destination="nchar(@Length)"></Type>
  <Type Source="ntext" Destination="oldtext"></Type>
  <Type Source="nvarchar(@Length)" Destination="nlongtext">
      <Length Operator="=">-1</Length>
  </Type>
  <Type Source="nvarchar(@Length)" Destination="nvarchar(@Length)"></Type>
  <Type Source="real" Destination="binaryfloat"></Type>
  <Type Source="smalldatetime" Destination="smalldatetime"></Type>
  <Type Source="smallmoney" Destination="smallmoney"></Type>
  <Type Source="text" Destination="oldtext"></Type>
  <Type Source="time" Destination="time"></Type>
  <Type Source="timestamp(@Length)" Destination="timestamp(@Length)"></Type>
  <Type Source="tinyint" Destination="int8"></Type>
  <Type Source="uniqueidentifier" Destination="guid"></Type>
  <Type Source="varbinary(@Length)" Destination="blob">
      <Length Operator="=">-1</Length>
  </Type>
  <Type Source="varbinary(@Length)" Destination="raw(@Length)"></Type>
  <Type Source="varchar(@Length)" Destination="longtext">
      <Length Operator="=">-1</Length>
  </Type>
  <Type Source="varchar(@Length)" Destination="varchar(@Length)"></Type>
</TypeConversions>
